        .rules-title {
            font-size: 1.8em;
            color: #fff;
            text-shadow: 0 2px 4px rgba(0, 0, 0, 0.3);
            font-family: 'Arial Rounded MT Bold', 'Arial', sans-serif;
            margin-bottom: 15px;
            letter-spacing: 1px;
            position: relative;
            padding-bottom: 10px;
        }
        .rules-title::after {
            content: '';
            position: absolute;
            bottom: 0;
            left: 0;
