            <div id="level">Level: 1</div>
            <div id="timer">Time: 0:00</div>
            <div id="date"></div>
        </div>

        <!-- Start Screen -->
        <div id="start-screen">
            <h1 class="game-title">Otter River Adventure</h1>
            
            <div class="menu-container">
                <div class="menu-left">
                    <div class="rules-container">
                        <h2 class="rules-title">How to Play</h2>
                        <ul class="rules-list">
                            <li>🎮 Use <strong>WASD</strong> or <strong>Arrow Keys</strong> to move</li>
                            <li>🎯 Collect fish and clams while avoiding obstacles</li>
                            <li>⚡ Game speeds up as you level up</li>
                            <li>✨ Rarer fish appear at higher levels</li>
                        </ul>
                    </div>
                </div>
                <div class="menu-right">
                    <div style="display: flex; flex-direction: column; gap: 20px;">
                        <div class="leaderboard">
                            <h2 class="leaderboard-title">Top Scores</h2>
                            <ul id="start-scores" class="leaderboard-list"></ul>
                        </div>
                        <div class="unlockables">
                            <h2 class="leaderboard-title">Unlockables</h2>
                            <div id="top-hat-unlock" class="unlockable-item" style="position: relative; display: flex; align-items: center; justify-content: space-between; background: rgba(0,0,0,0.7); border-radius: 10px; padding: 10px 15px; margin-top: 5px; min-height: 60px;">
                                <div style="position: absolute; left: 10px; top: 50%; transform: translateY(-50%); z-index: 0; opacity: 0.25;">
                                    <!-- Simple SVG otter with top hat -->
                                    <svg width="48" height="48" viewBox="0 0 48 48">
                                        <!-- Body -->
                                        <ellipse cx="24" cy="36" rx="12" ry="8" fill="#8B4513"/>
                                        <!-- Head -->
                                        <ellipse cx="24" cy="24" rx="8" ry="8" fill="#8B4513"/>
                                        <!-- Ears -->
                                        <ellipse cx="16" cy="20" rx="2" ry="2.5" fill="#8B4513"/>
                                        <ellipse cx="32" cy="20" rx="2" ry="2.5" fill="#8B4513"/>
                                        <!-- Eyes -->
                                        <ellipse cx="21" cy="25" rx="1" ry="1.2" fill="#000"/>
                                        <ellipse cx="27" cy="25" rx="1" ry="1.2" fill="#000"/>
                                        <!-- Nose -->
                                        <ellipse cx="24" cy="28" rx="1.2" ry="0.7" fill="#222"/>
                                        <!-- Top Hat Brim -->
                                        <ellipse cx="24" cy="16.5" rx="6" ry="1.2" fill="#111"/>
                                        <!-- Top Hat Body -->
                                        <rect x="18" y="10" width="12" height="7" rx="2" fill="#111"/>
                                        <!-- Top Hat Top -->
                                        <ellipse cx="24" cy="10" rx="6" ry="1.2" fill="#222"/>
                                    </svg>
                                </div>
                                <span style="font-size: 1.5em; margin-left: 60px; z-index: 1;">🎩 Top Hat</span>
                                <span id="top-hat-status" style="font-size: 1em; color: #ff00ff; z-index: 1;">Locked (Score 100+ in a game)</span>
                                <label for="top-hat-toggle" style="margin-left: 10px; z-index: 1;">Wear Top Hat:</label>
                                <input type="checkbox" id="top-hat-toggle" disabled>
                            </div>
                            <div class="unlockable-item" style="position: relative; display: flex; align-items: center; justify-content: space-between; background: rgba(0,0,0,0.7); border-radius: 10px; padding: 10px 15px; margin-top: 5px; min-height: 60px;">
                                <div style="position: absolute; left: 10px; top: 50%; transform: translateY(-50%); z-index: 0; opacity: 0.25;">
                                    <!-- Simple SVG otter with crown -->
                                    <svg width="48" height="48" viewBox="0 0 48 48">
                                        <!-- Body -->
                                        <ellipse cx="24" cy="36" rx="12" ry="8" fill="#8B4513"/>
                                        <!-- Head -->
                                        <ellipse cx="24" cy="24" rx="8" ry="8" fill="#8B4513"/>
                                        <!-- Ears -->
                                        <ellipse cx="16" cy="20" rx="2" ry="2.5" fill="#8B4513"/>
                                        <ellipse cx="32" cy="20" rx="2" ry="2.5" fill="#8B4513"/>
                                        <!-- Eyes -->
                                        <ellipse cx="21" cy="25" rx="1" ry="1.2" fill="#000"/>
                                        <ellipse cx="27" cy="25" rx="1" ry="1.2" fill="#000"/>
                                        <!-- Nose -->
                                        <ellipse cx="24" cy="28" rx="1.2" ry="0.7" fill="#222"/>
                                        <!-- Crown -->
                                        <path d="M18,16 L24,10 L30,16 L28,20 L20,20 Z" fill="#FFD700"/>
                                        <circle cx="24" cy="12" r="1" fill="#FFD700"/>
                                        <circle cx="20" cy="16" r="1" fill="#FFD700"/>
                                        <circle cx="28" cy="16" r="1" fill="#FFD700"/>
                                    </svg>
                                </div>
                                <span style="font-size: 1.5em; margin-left: 60px; z-index: 1;">👑 Crown</span>
                                <span id="crown-status" style="font-size: 1em; color: #ff00ff; z-index: 1;">Locked (Score 300+ in a game)</span>
                                <label for="crown-toggle" style="margin-left: 10px; z-index: 1;">Wear Crown:</label>
                                <input type="checkbox" id="crown-toggle" disabled>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <button class="start-button" onclick="startGame()">Start Adventure!</button>
            <p class="game-subtitle">A fun 3D game where you control an otter collecting fish and avoiding obstacles in a river!</p>
            <div class="points-container">
                <h2 class="points-title">Point Distribution</h2>
                <ul class="points-list">
                    <li>🐟 Normal Fish: 1 point</li>
                    <li>🌈 Rainbow Fish: 3 points</li>
                    <li>🌟 Golden Fish: 5 points</li>
                    <li>💎 Emerald Fish: 7 points</li>
                    <li>💠 Diamond Fish: 10 points</li>
                    <li>🦪 Clam: 15 points</li>
                    <li>🐠 Seahorse: 8 points</li>
                    <li>🐙 Jellyfish: 12 points</li>
                    <li>⭐ Starfish: 6 points</li>
                </ul>
            </div>
        </div>

        <!-- Game Over Screen -->
        <div id="game-over" style="display: none;">
            <a href="#" class="menu-link" onclick="returnToMenu()">← Back to Menu</a>
            <div class="game-over-content">
                <h1 class="game-title">Game Over!</h1>
                <div id="final-score">Final Score: 0</div>
                <div id="name-input">
                    <input type="text" id="player-name" placeholder="Enter your name" maxlength="20">
                    <button onclick="submitScore()">Submit Score</button>
                </div>
                <div class="leaderboard">
                    <h2 class="leaderboard-title">Top Scores</h2>
                    <ul id="top-scores" class="leaderboard-list"></ul>
                </div>
                <button class="start-button" onclick="restartGame()">Play Again</button>
            </div>
        </div>

        <!-- Controls Info -->
        <div class="controls-info">
            <p>⬆️ Up: W or ↑</p>
            <p>⬇️ Down: S or ↓</p>
            <p>⬅️ Left: A or ←</p>
            <p>➡️ Right: D or →</p>
        </div>

        <!-- Level Up Message -->
        <div id="level-up-message" style="display:none;position:fixed;top:50%;left:50%;transform:translate(-50%,-50%);z-index:200;font-size:3em;font-family:'Arial Rounded MT Bold',Arial,sans-serif;color:#fff;text-shadow:0 0 20px #00ffff,0 0 40px #ff00ff;background:rgba(0,0,0,0.7);padding:30px 60px;border-radius:30px;pointer-events:none;transition:opacity 0.5s;opacity:0;"></div>
    </div>

    <script>
        function returnToMenu() {
            document.getElementById('game-over').style.display = 'none';
            document.getElementById('start-screen').style.display = 'block';
            resetGameState();
            return false;
        }
    </script>
</body>
</html> 