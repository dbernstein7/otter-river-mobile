        .rules-list {
            list-style-type: none;
            padding: 0;
            display: flex;
            flex-direction: column;
            align-items: center;
            gap: 4px;
            width: 100%;
        }
        .rules-list li {
            margin: 0 auto;
            padding: 4px 10px;
            background: rgba(255, 255, 255, 0.1);
            border-radius: 6px;
            transition: all 0.3s ease;
            font-size: 0.9em;
            display: flex;
            align-items: center;
            transform: scale(1);
            width: 90%;
            max-width: 250px;
            text-align: left;
        }
        .rules-list li:hover {
            background: rgba(255, 255, 255, 0.2);
            transform: scale(1.05);
            box-shadow: 0 0 20px rgba(76, 175, 80, 0.2);
